{
  "name": "@escaladesports/esca-api-client",
  "version": "3.1.0",
  "description": "A JavaScript client library for Escalade Sports microservices",
  "main": "node/index.js",
  "browser": "browser/index.js",
  "scripts": {
    "build:node": "BABEL_ENV=node npx babel src --out-dir node",
    "build:browser": "BABEL_ENV=browser npx babel src --out-dir browser",
    "build": "npm run build:node && npm run build:browser",
    "test": "BABEL_ENV=node jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/escalade-internal/tree/master/packages/esca-api-client"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@escaladesports/babel-preset": "^2.7.0",
    "dotenv": "^8.2.0"
  },
  "keywords": [],
  "author": "Escalade Sports",
  "license": "ISC",
  "dependencies": {
    "@sentry/browser": "^5.13.2",
    "@sentry/node": "^5.13.2",
    "axios": "^0.19.2",
    "babel-plugin-module-rewrite": "^0.2.0",
    "lodash": "^4.17.15"
  }
}
